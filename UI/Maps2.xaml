<Window x:Class="AplikasiSepatu.UI.Maps2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AplikasiSepatu.UI"
        mc:Ignorable="d"
        Title="Maps2" Height="450" Width="450">
    <Grid>
        <Image Margin="-5,0,4,-1" Stretch="Fill" Source="/Assets/Maps.JPG" Height="598" VerticalAlignment="Bottom"/>
        <TextBlock HorizontalAlignment="Center" Margin="0,12,0,0" Text="Cari Lokasi Anda" TextWrapping="Wrap" VerticalAlignment="Top" Height="35" Width="204" Background="White" OpacityMask="Black" FontSize="14" FontWeight="Bold" TextAlignment="Center"/>
        <Image x:Name="Back" Margin="0,9,359,0" Stretch="Fill" Source="/Assets/BackButton.JPG" MouseLeftButtonDown="OnClickBack" Height="43" VerticalAlignment="Top" HorizontalAlignment="Right" Width="42" />
        <Image x:Name="Maps" Margin="104,223,102,0" Stretch="Fill" Source="/Assets/Map2.JPG" MouseLeftButtonDown="OnClickMap" Height="211" VerticalAlignment="Top"/>
    </Grid>
</Window>
