<Window x:Class="AplikasiSepatu.UI.Pemesanan"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AplikasiSepatu.UI"
        mc:Ignorable="d"
        Title="Pemesanan" Height="450" Width="450">
    <Grid Background="#FFFFF3F3">
        <Image x:Name="Peta" Margin="82,41,69,0" Stretch="Fill" Source="/Assets/Tempat.JPG" Height="74" VerticalAlignment="Top" MouseLeftButtonDown="OnClickPeta"/>
        <Image x:Name="Back" Margin="1,-2,0,0" Stretch="Fill" Source="/Assets/Pemesanan.JPG" Height="45" VerticalAlignment="Top" HorizontalAlignment="Left" Width="233" MouseLeftButtonDown="OnClickBack"/>
        <Image Margin="120,0,0,0" Stretch="Fill" Source="/Assets/Detail.JPG" Height="241" VerticalAlignment="Center" HorizontalAlignment="Left" Width="214"/>
        <Image x:Name="Checkout" Margin="122,0,104,37" Stretch="Fill" Source="/Assets/Checkout.JPG" Height="39" VerticalAlignment="Bottom" MouseLeftButtonDown="OnClickCheckOut"/>

    </Grid>
</Window>
